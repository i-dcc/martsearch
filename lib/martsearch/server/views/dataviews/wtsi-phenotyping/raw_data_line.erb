<div id="wtsi-phenotyping-report" class="wtsi-phenotyping">
	<table border='0' cellpadding='0' cellspacing='0'>
		<tbody>
		  <% @data[:observations].each do |observation| %>
		    <% @data[:genders].each do |gender| %>
  		    <tr>
  		      <td>
  		        <div><%= gender %><%= observation %></div>
  		        <table>
  		          <thead>
  		            <tr>
  		              <thead>
  		                <% @data[:titles].each do |title| %>
  		                  <th><%= title %></th>
  		                <% end %>
  		              </thead>
  		            </tr>
  		          </thead>
  		        </table>
  		        <td>
  		    </tr>
		    <% end %>
		  <% end %>
		</tbody>
	</table>
</div>
