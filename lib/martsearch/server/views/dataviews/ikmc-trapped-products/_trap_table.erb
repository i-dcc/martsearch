<table>
  <caption><%= caption %></caption>
  <tr>
    <th><%= first_cell_header %></th>
    <th>Tag Type</th>
    <th>Vector</th>
    <th>ES Cell Clone</th>
    <th>ES Cell Line</th>
    <th>ES Cell Strain</th>
    <th>Order</th>
  </tr>
  <% traps.each_index do |index| %>
    <%
      row_class = "even"
      row_class = "odd" if index % 2 == 0
      trap      = traps[index]
    %>
    <tr class="<%= row_class %>">
      <td><%= trap[first_cell_content.to_sym]%></td>
      <td><%= trap[:design_type] %></td>
      <td><%= trap[:vector_name] %></td>
      <td><%= trap[:escell_clone] %></td>
      <td><%= trap[:escell_line] %></td>
      <td><%= trap[:escell_strain] %></td>
      <td>
        <%=
          escell_order_button(
            mgi_acc_id,
            marker_symbol,
            trap[:project],
            nil,
	    trap[:escell_clone]
          )
        %>
      </td>
    </tr>
  <% end %>
</table>