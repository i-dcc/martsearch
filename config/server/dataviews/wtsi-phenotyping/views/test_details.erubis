<div id="wtsi-phenotyping-report">
  <h2><%= @test_name %> Data for <%= @marker_symbol %> (<%= @colony_prefix %>)</h2>

  <% @data[:significant].each do |protocol_desc_hash,protocol_data| %>
    <div id="test-overview" class="test-descriptor">
      <h4>Overview</h4>
      <div class="test-descriptor-content">
        <%= protocol_data[:protocol_description] %>
      </div>
    </div>
    
    <div id="test-images">
      <% protocol_data[:parameters].each do |order_by,parameter| %>
        <img src="<%= parameter[:graph_url] %>" alt="<%= parameter[:parameter] %>" />
      <% end %>
    </div>
  <% end %>
</div>
