<div id="wtsi-phenotyping-report" class="wtsi-phenotyping">
  <h2><%= @test_name %> Data for <%= @marker_symbol %> (<%= @colony_prefix %>)</h2>
  
  <% if @data[:protocol_data].empty? %>
    <div><em>No data reported</em></div>
  <% else %>
    <div class="accordion active">
      <% @data[:protocol_data].sort_by{ |k,v| v[:order_by] }.each do |protocol_desc_hash,protocol_data| %>
        <% title = protocol_data[:protocol] %>
        <%= 
          partial(
            'dataviews/wtsi-phenotyping/test_details_protocol_graphs', 
            :locals => { 
              :title         => title,
              :protocol_data => protocol_data
            }
          ) 
        %>
      <% end %>
    </div>
  <% end %>
</div>
