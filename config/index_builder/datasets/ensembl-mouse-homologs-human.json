{
  "datasource": "ensembl-mouse",
  "indexing": {
    "allow_document_creation": false,
    "days_between_downlads": 30,
    "attribute_map": [
      { "attr": "external_gene_id",                  "idx": "marker_symbol", "use_to_map": true },
      { "attr": "ensembl_gene_id",                   "idx": "ensembl_gene_id" },
      { "attr": "ensembl_transcript_id",             "idx": "ensembl_transcript_id" },
      { "attr": "ensembl_peptide_id",                "idx": "ensembl_peptide_id" }
    ]
  }
}